
<strong><%= rating.to_s %></strong> / <%= maxRating %>

<script>
    $(function() {
        
        $(".review-rating").raty ({
            readOnly: true,
            score: function() {
                return $(this).attr("data-score");
            },
            path: "<%= @star_path %>" // "app/assets/images/" // "/assets/"
        });


    });
</script>